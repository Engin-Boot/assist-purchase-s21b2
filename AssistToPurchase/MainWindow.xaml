<Window x:Class="AssistToPurchase.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AssistToPurchase"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" WindowState="Normal" WindowStyle="SingleBorderWindow">
    <ScrollViewer HorizontalScrollBarVisibility="Visible" VerticalScrollBarVisibility="Visible">
        <Frame x:Name="frame" NavigationUIVisibility="Visible" HorizontalAlignment="Center" VerticalAlignment="Center" ></Frame>
    </ScrollViewer>
</Window>
