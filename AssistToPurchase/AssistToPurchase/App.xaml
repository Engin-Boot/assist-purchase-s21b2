<Application x:Class="AssistToPurchase.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AssistToPurchase"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Resources/styles/MainWindow.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/Resources/ContinuousMonitoringTemplate.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/Resources/SaleRepresentativeDataTemplate.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
